package com.litl.weather.model.twc{    import com.litl.weather.model.twc.Bar;    import com.litl.weather.model.twc.Wind;    import com.litl.weather.model.twc.UV;    import com.litl.weather.model.twc.Moon;    public class CC    {        // Public Properties:        // Private Properties:        private var _lsup:String; // ?        private var _obst:String; // observation station name        private var _tmp:String; // temperature        private var _flik:String; // "feels like" temperature	        private var _t:String; // weather description phrase        private var _icon:String; // weather icon code        private var _bar:Bar; // barometric pressure and trend        private var _wind:Wind; // wind speed, gust, direction, and phrase        private var _hmid:String; // relative humidity        private var _vis:String; // visibility        private var _uv:UV; // UV index and description        private var _dewp:String; // dew point        private var _moon:Moon; // moon phase description and icon code        // Initialization:        public function CC() {            bar = new Bar();            wind = new Wind();            uv = new UV();            moon = new Moon();        }        // Public Methods:        public function get lsup():String {            return _lsup;        }        public function set lsup(lsup:String):void {            _lsup = lsup;        }        public function get obst():String {            return _obst;        }        public function set obst(obst:String):void {            _obst = obst;        }        public function get tmp():String {            return _tmp;        }        public function set tmp(tmp:String):void {            _tmp = tmp;        }        public function get flik():String {            return _flik;        }        public function set flik(flik:String):void {            _flik = flik;        }        public function get t():String {            return _t;        }        public function set t(t:String):void {            _t = t;        }        public function get icon():String {            return _icon;        }        public function set icon(icon:String):void {            _icon = icon;        }        public function get bar():Bar {            return _bar;        }        public function set bar(bar:Bar):void {            _bar = bar;        }        public function get wind():Wind {            return _wind;        }        public function set wind(wind:Wind):void {            _wind = wind;        }        public function get hmid():String {            return _hmid;        }        public function set hmid(hmid:String):void {            _hmid = hmid;        }        public function get vis():String {            return _vis;        }        public function set vis(vis:String):void {            _vis = vis;        }        public function get uv():UV {            return _uv;        }        public function set uv(uv:UV):void {            _uv = uv;        }        public function get dewp():String {            return _dewp;        }        public function set dewp(dewp:String):void {            _dewp = dewp;        }        public function get moon():Moon {            return _moon;        }        public function set moon(moon:Moon):void {            _moon = moon;        }        public function toString():String {            return "CC: " +                "\n lsup: " + lsup +                "\n obst: " + obst +                "\n tmp: " + tmp +                "\n flik: " + flik +                "\n t: " + t +                "\n icon: " + icon +                "\n " + bar +                "\n " + wind +                "\n hmid: " + hmid +                "\n vis: " + vis +                "\n " + uv +                "\n dewp: " + dewp +                "\n " + moon +                "\n";        }        // Private Methods:    }}