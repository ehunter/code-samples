package com.litl.weather.model.twc{    public class Lnks    {        // Public Properties:        // Private Properties:        private var _type:String; // ex. "prmo"        private var _link:Array; // of TWCLink        // Initialization:        public function Lnks() {            _link = new Array();        }        // Public Methods:        public function get type():String {            return _type;        }        public function set type(type:String):void {            _type = type;        }        public function get link():Array {            return _link;        }        public function set link(link:Array):void {            _link = link;        }        public function toString():String {            return "Lnks: " +                "\n type: " + type +                "\n " + link +                "\n";        }        // Private Methods:    }}