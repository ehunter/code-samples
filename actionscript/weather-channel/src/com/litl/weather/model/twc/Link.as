package com.litl.weather.model.twc{    public class Link    {        // Public Properties:        // Private Properties:        private var _pos:String; // position        private var _l:String; // link URL        private var _t:String; // text description        // Initialization:        public function Link() {        }        // Public Methods:        public function get pos():String {            return _pos;        }        public function set pos(pos:String):void {            _pos = pos;        }        public function get l():String {            return _l;        }        public function set l(l:String):void {            _l = l;        }        public function get t():String {            return _t;        }        public function set t(t:String):void {            _t = t;        }        public function toString():String {            return "Link: " +                "\n pos: " + pos +                "\n l: " + l +                "\n t: " + t +                "\n";        }        // Private Methods:    }}