package com.litl.weather.model.twc{    import com.litl.weather.model.twc.Wind;    public class Part    {        // Public Properties:        // Private Properties:        private var _icon:String;        private var _t:String;        private var _wind:Wind;        private var _bt:String;        private var _ppcp:String;        private var _hmid:String;        // Initialization:        public function Part() {            wind = new Wind();        }        // Public Methods:        public function get icon():String {            return _icon;        }        public function set icon(icon:String):void {            _icon = icon;        }        public function get t():String {            return _t;        }        public function set t(t:String):void {            _t = t;        }        public function get wind():Wind {            return _wind;        }        public function set wind(wind:Wind):void {            _wind = wind;        }        public function get bt():String {            return _bt;        }        public function set bt(bt:String):void {            _bt = bt;        }        public function get ppcp():String {            return _ppcp;        }        public function set ppcp(ppcp:String):void {            _ppcp = ppcp;        }        public function get hmid():String {            return _hmid;        }        public function set hmid(hmid:String):void {            _hmid = hmid;        }        public function toString():String {            return "Part: " +                "\n icon: " + icon +                "\n t: " + t +                "\n wind: " + wind +                "\n bt: " + bt +                "\n ppcp: " + ppcp +                "\n hmid: " + hmid +                "\n";        }        // Private Methods:    }}